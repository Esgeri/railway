<tr>
  <td>
    <div id="route_title_<%= route.id %>">
      <%= route.name %>
    </div>
    <%= render 'form', route: route, hidden: true %>
  </td>
  <td>
    <%= link_to t('common.show'), [ :admin, route ] %>
    <%= link_to t('common.edit'), ' ', class: 'edit_route', data: { route_id: route.id } %>
    <%= link_to t('common.destroy'), [ :admin, route ], method: :delete, data: { confirm: t('common.confirm') } %>
  </td>
</tr>
